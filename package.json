{
  "name": "@groceristar/grocery-component",
  "version": "1.0.10",
  "description": "module to use grocery components between projects",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/GroceriStar/grocery-react-component",
  "author": "Arthur Tkachenko",
  "license": "MIT",
  "private": false,
  "scripts": {
    "bundle": "rollup -c",
    "build": "babel src -d dist",
    "build:test": "shx rm -rf && babel src -d dist",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "lint": "standard",
    "code-fix": "standard --fix",
    "zprepublish": "yarn clean && yarn build",
    "test": "CI=true && jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^24.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "shx": "^0.3.2",
    "sinon": "^9.0.1",
    "standard": "^14.1.0"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "files": [
    "dist"
  ]
}
